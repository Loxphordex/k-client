{
  "name": "k-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.11.0",
    "bufferutil": "^4.0.2",
    "canvas": "^2.6.1",
    "cloudinary-react": "^1.1.4",
    "dotenv": "^8.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.2",
    "es6-promise": "^4.2.8",
    "html-react-parser": "^1.2.1",
    "isomorphic-fetch": "^3.0.0",
    "msw": "^0.30.0",
    "node-sass": "^4.14.1",
    "phosphor-react": "^1.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-quill": "^1.3.5",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^4.0.0",
    "react-snapshot": "^1.3.0",
    "sass": "^1.29.0",
    "ts-node": "^9.0.0",
    "type-fest": "^0.13.1",
    "typescript": "^3.9.7",
    "utf-8-validate": "^5.0.3",
    "uuid": "^8.3.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-plugin-serve": "^0.12.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "predeploy": "npm run build",
    "deploy": "now ./build",
    "postdeploy": "now alias -A ./build/now.json"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "enzyme"
    ],
    "rules": {
      "react/prop-types": 0,
      "react/no-unescaped-entities": 0,
      "no-unused-vars": 1
    },
    "settings": {
      "react": {
        "createClass": "createReactClass",
        "pragma": "React",
        "fragment": "Fragment",
        "version": "detect",
        "flowVersion": "0.53"
      },
      "propWrapperFunctions": [
        "forbidExtraProps",
        {
          "property": "freeze",
          "object": "Object"
        },
        {
          "property": "myFavoriteWrapper"
        }
      ],
      "linkComponents": [
        "Hyperlink",
        {
          "name": "Link",
          "linkAttribute": "to"
        }
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/config/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/src/config/setupJest.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/config/setupTests.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 and_chr versions",
      "last 3 chrome versions",
      "last 3 opera versions",
      "last 3 ios_saf versions",
      "last 3 safari versions"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.23",
    "babel-jest": "^26.6.3",
    "chai": "^4.3.4",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-equimper": "*",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-enzyme": "^0.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jasmine": "^4.1.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^1.14.3",
    "react-test-renderer": "^16.14.0",
    "regenerator-runtime": "^0.13.7"
  }
}
