  "dependencies": {
    "cloudinary-react": "^1.1.4",
    "dotenv": "^8.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.2",
    "es6-promise": "^4.2.8",
    "isomorphic-fetch": "^3.0.0",
    "phosphor-react": "^1.0.0",
