    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-equimper": "*",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.21.2",
