{
    "version": 2,
    "alias": "pearegrine",
    "name": "pearegrine",
    "routes": [
        {
          "src": "^/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": ".*",
          "dest": "/index.html"
        },
        {
          "src": "^/favicon.ico",
          "dest": "/favicon.ico"
        },
        {
          "src": "^/manifest.json",
          "dest": "/manifest.json"
        }
    ]
}